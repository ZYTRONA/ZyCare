{
  "expo": {
    "name": "ZYCARE",
    "slug": "zycare",
    "extra": {
      "EXPO_PUBLIC_API_URL": "http://10.56.198.1:5000",
      "EXPO_PUBLIC_SOCKET_URL": "http://10.56.198.1:5000",
      "EXPO_PUBLIC_AI_ENGINE_URL": "http://10.56.198.1:8000"
    },
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
